<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::stores">
                    <resource id="MGS_OSCheckout::onestepcheckout" title="One Step Checkout" sortOrder="45">
                        <resource id="MGS_OSCheckout::field_management" title="Manage Fields" sortOrder="10"/>
                    </resource>
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MGS_OSCheckout::configuration" title="magesolution One Step Checkout"/>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
